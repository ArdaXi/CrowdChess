{
  "include": [
    "src/*.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noEmitOnError": false,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "target": "es5"
  }
}
